  FROM  order_opt_history 
  WHERE  order_common_seqno = '76'\n\n\nquery :  SELECT /* 상업인쇄 조판대기 리스트 */  A.order_detail_file_num, C.title, C.order_common_seqno, F.sortcode, F.cate_name, E.member_id, E.member_name, B.side_dvs, B.tot_tmpt, B.amt, G.name, G.color, G.basisweight, C.order_regi_date  ,A.file_path, A.save_file_name, C.amt_unit_dvs, B.stan_name, B.receipt_finish_date, B.produce_memo  from order_detail_count_file AS A  INNER JOIN order_detail AS B ON A.order_detail_seqno = B.order_detail_seqno  INNER JOIN order_common AS C ON B.order_common_seqno = C.order_common_seqno  INNER JOIN amt_order_detail_sheet AS D ON D.order_detail_count_file_seqno = A.order_detail_count_file_seqno  INNER JOIN member AS E ON E.member_seqno = C.member_seqno  INNER JOIN cate AS F ON C.cate_sortcode = F.sortcode  INNER JOIN cate_paper AS G ON G.mpcode = B.cate_paper_mpcode  WHERE D.sheet_typset_seqno IS NULL and C.commerce_dvs = '상업' and C.order_state = '2120' \n\n\nquery : 
