query : SELECT /* 수동접수 - 작업목록 얻기 */ A.order_num, A.title, A.cate_sortcode, A.order_detail, A.order_common_seqno, A.order_state  ,B.member_id, B.member_name, G.cate_name, B.onefile_etprs_yn  ,C.side_dvs, A.count ,A.order_regi_date, C.receipt_mng, A.pay_price  FROM order_common AS A  INNER JOIN member AS B ON A.member_seqno = B.member_seqno  INNER JOIN order_detail AS C ON A.order_common_seqno = C.order_common_seqno  INNER JOIN order_file AS D ON A.order_common_seqno = D.order_common_seqno  INNER JOIN member_mng AS E ON A.member_seqno = E.member_seqno  INNER JOIN empl AS F ON F.empl_seqno = E.mac  INNER JOIN cate AS G ON A.cate_sortcode = G.sortcode  WHERE D.origin_file_name LIKE '%.sit'  AND (A.order_state = '1320' OR ((A.order_state = '1330' OR A.order_state = '1370') AND C.receipt_mng = 'test3')) AND A.receipt_dvs = 'Manual'  AND A.order_num like '%170810%'  AND (C.receipt_mng = 'test3' OR C.receipt_mng = '' OR C.receipt_mng IS NULL)  ORDER BY  CASE  WHEN F.empl_id = 'test3' then 1  ELSE 2 END , A.order_regi_date  LIMIT 20 \n\n\nquery : 
