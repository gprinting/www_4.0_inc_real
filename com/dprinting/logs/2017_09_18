  WHERE  order_common_seqno IN ('76','116','146','167','194','196','240','244','306','362','382','387','411','438','443','454','535','558','664','735')\n\n\nquery :  SELECT /* 수동접수 - 주문상세정보 */  A.order_num, A.title, A.order_common_seqno, A.order_state, A.count, A.amt_unit_dvs, A.receipt_finish_date,   A.cate_sortcode, C.cate_name, B.order_detail_dvs_num, B.typset_way,  E.member_id, E.member_name,  A.count, A.order_regi_date, D.name, D.color, D.basisweight,  B.work_size_wid, B.work_size_vert, B.cut_size_wid, B.cut_size_vert,  B.stan_name, B.side_dvs, B.tot_tmpt, B.amt, A.cust_memo, F.file_path, F.save_file_name, B.receipt_mng, B.receipt_memo,  G.dlvr_way, G.dlvr_sum_way, H.name as empl_name  FROM order_common AS A  INNER JOIN order_detail AS B ON A.order_common_seqno = B.order_common_seqno  INNER JOIN cate AS C ON A.cate_sortcode = C.sortcode  INNER JOIN cate_paper AS D ON B.cate_paper_mpcode = D.mpcode  INNER JOIN member AS E ON A.member_seqno = E.member_seqno  INNER JOIN order_file AS F ON A.order_common_seqno = F.order_common_seqno  LEFT JOIN order_dlvr AS G ON A.order_common_seqno = G.order_common_seqno AND G.tsrs_dvs = '수신'  LEFT JOIN empl AS H ON B.receipt_mng = H.empl_id  WHERE A.order_num = %s \n\n\nquery : 
